								"exec": [
									"// Определяем, успешный ли ответ или ошибка\r",
									"if (pm.response.code === 200) {\r",
									"    // ТЕСТЫ ДЛЯ УСПЕШНОГО ЛОГИНА\r",
									"    pm.test(\"✅ Status code is 200\", function () {\r",
									"        pm.response.to.have.status(200);\r",
									"    });\r",
									"    \r",
									"    pm.test(\"✅ Response has auth token\", function () {\r",
									"        var jsonData = pm.response.json();\r",
									"        pm.expect(jsonData.token || jsonData.access_token).to.not.be.undefined;\r",
									"    });\r",
									"    \r",
									"    pm.test(\"✅ Save auth data\", function () {\r",
									"        var jsonData = pm.response.json();\r",
									"        var token = jsonData.token || jsonData.access_token;\r",
									"        pm.environment.set(\"auth_token\", token);\r",
									"        pm.environment.set(\"user_id\", jsonData.user_id);\r",
									"        console.log(\"Token saved for user ID:\", jsonData.user_id);\r",
									"    });\r",
									"    \r",
									"} else if (pm.response.code === 401) {\r",
									"    // ТЕСТЫ ДЛЯ ОШИБКИ 401\r",
									"    pm.test(\"❌ Status code is 401 Unauthorized\", function () {\r",
									"        pm.response.to.have.status(401);\r",
									"    });\r",
									"    \r",
									"    pm.test(\"❌ Response has error details\", function () {\r",
									"        var jsonData = pm.response.json();\r",
									"        pm.expect(jsonData.error).to.not.be.undefined;\r",
									"        console.log(\"Auth error:\", jsonData.error);\r",
									"    });\r",
									"    \r",
									"    pm.test(\"❌ Clear old auth data\", function () {\r",
									"        pm.environment.unset(\"auth_token\");\r",
									"        pm.environment.unset(\"user_id\");\r",
									"    });\r",
									"    \r",
									"} else {\r",
									"    // ТЕСТЫ ДЛЯ ДРУГИХ ОШИБОК\r",
									"    pm.test(`Unexpected status: ${pm.response.code}`, function () {\r",
									"        console.log(\"Unexpected response:\", pm.response.text());\r",
